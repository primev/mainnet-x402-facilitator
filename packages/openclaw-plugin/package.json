{
  "name": "@primev/openclaw-plugin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "openclaw": {
    "extensions": [
      "dist/index.js"
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && node ./scripts/validate-manifest.mjs",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test test/**/*.test.ts && node ./scripts/validate-manifest.mjs",
    "lint": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
